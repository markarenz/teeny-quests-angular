{
  "name": "teeny-quests-angular",
  "version": "0.9.53",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "deploy": "aws s3 sync dist/teeny-quests-angular/browser s3://teeny-quests",
    "build:deploy": "npm run build && npm run deploy",
    "watch": "ng build --watch --configuration development",
    "lambda:deploy": "cd ./api/src && zip -r ../lambda.zip ./* && cd .. && aws lambda update-function-code --function-name TQ-SharedApiLambda --zip-file fileb://lambda.zip --region us-east-1 && cd ..",
    "test": "ng test --code-coverage --browsers=ChromeHeadless --no-watch",
    "test:watch": "ng test --code-coverage --browsers=ChromeHeadless --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "angular-oauth2-oidc": "^17.0.2",
    "ngx-toastr": "^19.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^11.0.5",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.9",
    "@angular/cli": "^18.2.9",
    "@angular/compiler-cli": "^18.2.0",
    "@types/jasmine": "~5.1.0",
    "autoprefixer": "^10.4.20",
    "fetch-mock": "^12.2.0",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.47",
    "prettier": "3.6.2",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.5.2"
  }
}
